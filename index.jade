head
    title Survial Clicker
    meta(charset="UTF-8")
    link(rel="stylesheet", type="text/css", href="bootstrap.min.css")
body
    .container-fluid
        .row: .col-lg-12
            label Version:
            label.text-success 0.3
            label(id="savegamename").text-warning dev
            a.pull-right(target="_blank", href="https://www.reddit.com/r/incremental_games/comments/38t4o1/survival_clicker_12hr_of_work/") Official Reddit Thread
        .row: .col-lg-12
            label Money:
            label(id="money")
            label $
            label (
            label(id="income")
            label $/s)
        .row: .col-lg-12
            label Taxes:
            label(id="taxes")
            label %
        .row: .col-lg-12
            label Score:
            label.text-danger Alive For:
            label.text-success(id="time")
            label.text-danger Digested:
            label.text-success(id="digested")
        .row: .col-lg-12
            label.text-primary Body Temperature:
            label.label-success(id="temp") Normal
            label.text-primary Clothing Temperature Gain:
            label(id="tempoffset")
        .row: .col-lg-12
            label Health:
            .progress: .progress-bar.progress-bar-danger(id="health")
        .row: .col-lg-12
            label Stamina:
            .progress: .progress-bar(id="stamina")
        .row: .col-lg-12
            label Stomach:
            .progress: .progress-bar.progress-bar-warning(id="stomach")
        .row: .col-lg-12
            label Energy:
            .progress: .progress-bar.progress-bar-success(id="energy")
        .row: .col-lg-12
            label Hydration:
            .progress: .progress-bar.progress-bar-info(id="hydration")
        .row: .col-lg-12
            .form-group
            label Thoughts:
            inout.form-control(id="log", readonly)
        p
        .row: .col-lg-12
            button.btn.btn-default(id="pause") Pause
        p
        .row: .col-lg-12
            ul.nav.nav-tabs
                li.active: a(href="#common", data-toggle="tab") Common Actions
                li: a(href="#work", data-toggle="tab") Work
                li: a(href="#training" , data-toggle="tab") Training
                li: a.consumable(money="-5", href="#clothing" , data-toggle="tab") Clothing
                li: a(href="#food" , data-toggle="tab") Food
                li: a(href="#drink" , data-toggle="tab") Drink
                li: a.consumable(money="-1", href="#investment" , data-toggle="tab") Investment
                li: a.consumable(money="-50", href="#chems" , data-toggle="tab") Chems(dangerous)
                li: a.consumable(money="-500", href="#black" , data-toggle="tab") Black Market Auto Injectors(deadly)
                li: a.consumable(money="-2000", href="#medical" , data-toggle="tab") Medical Services(safe)
                li: a.consumable(money="-60000", href="#bribe" , data-toggle="tab") Bribe Gov
                li: a.consumable(money="-75000", href="#et" , data-toggle="tab") ET Technology
                li: a.consumable(money="-1000000", href="#research" , data-toggle="tab") Research
        p
        .row: .col-lg-12
            .tab-content
                .tab-pane.active(id="common")
                    button.btn.btn-default(id="reset") Suicide
                    button.btn.btn-default.consumable.hide(money="50", hydration="-50", energy="-50", stomachcap="-50", healthcap="-10", title="Can be dangerous") Participate in human experiment
                .tab-pane(id="work")
                    button.btn.btn-default.consumable(money="5", hydration="-3", energy="-2", stamina="-5", staminacap="0.02", temp="0.05", title="Standard Work") Basic Work
                    button.btn.btn-default.consumable.buyable(buyname="bricks", money="10", hydration="-4", energy="-2.5", stamina="-6", staminacap="0.021", temp="0.06") Carry Bricks
                    button.btn.btn-default.consumable.buyable(buyname="equipm", money="15", hydration="-5", energy="-3.5", stamina="-7", staminacap="0.022", temp="0.07") Make Concrete
                    button.btn.btn-default.consumable.buyable(buyname="progrm", money="20", hydration="-1", energy="-2.5", stamina="-0.2", staminacap="-0.005", temp="0.001") Program Websites
                    button.btn.btn-default.consumable.buyable(buyname="sprayc", money="30", hydration="-5.5", energy="-4", stamina="-6", staminacap="-0.004", healthcap="-0.0001", temp="0.05") Spray Crops
                    button.btn.btn-default.consumable.buyable(buyname="relig", money="40", hydration="-2.5", energy="-2", stamina="-3", staminacap="0.0004", temp="0.08") Force Religion on People
                    button.btn.btn-default.consumable.buyable(buyname="pccomp", money="150", hydration="-1.5", energy="-2", stamina="-0.5", staminacap="-0.001", temp="-0.001") Research Computers
                    p
                    button.btn.btn-default.consumable(money="-500", unlock="bricks") Look for a better job
                    button.btn.btn-default.consumable(money="-2000", unlock="equipm") Invest into Concrete Mixer
                    button.btn.btn-default.consumable(money="-5000", unlock="progrm") Learn to Program
                    button.btn.btn-default.consumable(money="-10000", unlock="sprayc") Invest into Crop Spray Equipment
                    button.btn.btn-default.consumable(money="-2500000", unlock="relig") Create a new Religion
                    button.btn.btn-default.consumable(money="-25000000", unlock="pccomp") Create a big computer company
                .tab-pane(id="training")
                    button.btn.btn-default.consumable(money="-7", hydration="-1", energy="-10", stamina="-40", staminacap="0.1", staminaregen="0.00001", temp="-0.05", title="Improves your maximum stamina also cools you down") Swim in a pool
                    button.btn.btn-default.consumable(money="-4", hydration="-7", energy="-5", stamina="-15", staminacap="0.09", staminaregen="0.00001", temp="0.05", title="Improves your maximum stamina") Workout on strength
                    button.btn.btn-default.consumable(money="-4", hydration="-7.5", energy="-5", stamina="-15", staminacap="0.0001", staminaregen="0.001", temp="0.04", title="Improves your stamina regeneration") Workout on breathing
                    button.btn.btn-default.consumable(money="-50", stamina="-1", energy="-1", digestionmulti="0.001", stomachcapin="0.000001", stomachcap="0.001", temp="0.01", title="Improves digestion speed and capacity") Stomach Massage
                    button.btn.btn-default.consumable(money="-500", stamina="-5", energy="-10", digestionmulti="0.002", stomachcapin="0.000003", stomachcap="0.01", temp="0.01", title="Improves digestion speed and capacity") Stomach Massage Extended
                .tab-pane(id="clothing")
                    button.btn.btn-default.consumable(money="-5", tempoffset="0") Normal Clothes
                    button.btn.btn-default.consumable(money="-15", tempoffset="-0.01") Loose Clothes
                    button.btn.btn-default.consumable(money="-10", tempoffset="0.01") Warm Clothes
                    button.btn.btn-default.consumable(money="-50", tempoffset="0.02") Quality Warm Clothes
                    button.btn.btn-default.consumable(money="-75", tempoffset="-0.02") Quality Loose Clothes
