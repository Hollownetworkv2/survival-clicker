                      button.btn.btn-default.consumable(money="-10000000000000", moneyincome="100000000000", title="Get 100b$ per second") Invest
                 .tab-pane(id="chems")
                      button.btn.btn-default.consumable(money="-30", stamina="-10", health="-0.5", healthcap="-0.01", healthcaploss="0.001", staminacap="-0.02", staminaregen="-0.0002", temp="-1", title="Lowers body temperature") Spray Old Freon On Self
                      button.btn.btn-default.consumable(money="-30", stamina="-50", health="-0.5", healthcap="-0.05", healthcaploss="0.002", staminacap="-0.2", staminaregen="-0.002", temp="-3", title="Lowers body temperature") Drink Old Freon On Self
                      button.btn.btn-default.consumable(money="-40", stomach="25", stomachcap="-1", stamina="50", health="-0.5", healthcap="-0.01", staminacap="-0.02", staminaregen="-0.0002", temp="0.01", title="Improves stamina with little effort") Stamina Restorer Milk
