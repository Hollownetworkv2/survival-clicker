                    button.btn.btn-default.consumable(money="-10000", energy="12.5", hydration="0.45", stomach="3", health="-0.0000001") Pro Engineered Food From ETs
                    button.btn.btn-default.consumable(money="-100000", energy="13.5", hydration="0.48", stomach="2.9", health="-0.00000001") Pro Engineered Food From ETs (Other Galaxy)
                 .tab-pane(id="drink")
                     button.btn.btn-default.consumable(money="0", hydration="1", stomach="9.5", health="-0.5") Drain Water
                     button.btn.btn-default.consumable(money="-1", hydration="5", stomach="4.5", health="-0.08") Suspicious Water
                     button.btn.btn-default.consumable(money="-1.25", hydration="1.5", energy="2", stomach="3.5", health="-0.15") Energy Drink
                     button.btn.btn-default.consumable(money="-1.5", hydration="10", stomach="3.5", health="-0.01") Water
                     button.btn.btn-default.consumable(money="-5", hydration="11", stomach="3", health="-0.001") Quality Water
                     button.btn.btn-default.consumable(money="-20", hydration="12", stomach="2.5", health="-0.0001") Premium Water
                     button.btn.btn-default.consumable(money="-125", hydration="13", stomach="2.25", health="-0.00001") Pro Engineered Water
                     button.btn.btn-default.consumable(money="-800", hydration="14", stomach="2.05", health="-0.000001") Pro Engineered Water From Space
                     button.btn.btn-default.consumable(money="-10000", hydration="15", stomach="2", health="-0.0000001") Pro Engineered Water From ETs
                     button.btn.btn-default.consumable(money="-100000", hydration="16", stomach="1.95", health="-0.00000001") Pro Engineered Water From ETs (Other Galaxy)
                  .tab-pane(id="investment")
                      button.btn.btn-default.consumable(money="-1", moneyincome="0.01", title="Get 0.01$ per second") Invest
