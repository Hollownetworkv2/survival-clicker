                    button.btn.btn-default.consumable(money="-150", tempoffset="-0.03") Premium Loose Clothes
                    button.btn.btn-default.consumable(money="-1000", tempoffset="0.1") Gillie Suit
                    button.btn.btn-default.consumable(money="-1000000", tempoffset="0.2") Space Suit
                    button.btn.btn-default.consumable(money="-10000000", tempoffset="1") Silicone Seal
                .tab-pane(id="food")
                    button.btn.btn-default.consumable(money="0", energy="1", hydration="-0.1", stomach="11", health="-0.7") Discarded Food
                    button.btn.btn-default.consumable(money="-3", energy="4", hydration="0.05", stomach="7", health="-0.01") Cheap Food
                    button.btn.btn-default.consumable(money="-6", energy="5", hydration="0.06", stomach="5", health="-0.005") Decent Food
                    button.btn.btn-default.consumable(money="-10", energy="7", hydration="0.08", stomach="5", health="-0.002") Food
                    button.btn.btn-default.consumable(money="-20", energy="8.5", hydration="0.09", stomach="4.8", health="-0.001") Quality Food
